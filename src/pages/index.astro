    <meta name="viewport" content="width=device-width">
    <title>{title}</title>

    <link rel="icon" type="image/x-icon" href="/favicon.ico" />

    <link rel="stylesheet" href="/style/global.css">
    <link rel="stylesheet" href="/style/home.css">

    <style>

    </style>
</head>
<body>
  <main>
    <Header
       client:visible
      titles={titles}
    />
  </main>
</body>
</html>
